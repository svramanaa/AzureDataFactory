{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "svramanaa-adf"
		},
		"ls_employee_file_data_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_employee_file_data'"
		},
		"ls_hrdata_files_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_hrdata_files'"
		},
		"ls_sql_employee_data_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_employee_data'"
		},
		"ls_sql_hrdata_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_hrdata'"
		},
		"ls_sql_employee_data_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "svr-staging-db.database.windows.net"
		},
		"ls_sql_employee_data_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "rsv-staging-db"
		},
		"ls_sql_employee_data_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "svramanaa"
		},
		"ls_sql_hrdata_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "svr-staging-db.database.windows.net"
		},
		"ls_sql_hrdata_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "rsv-staging-db"
		},
		"ls_sql_hrdata_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "svramanaa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "svramanaa-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_employee_file_data_connectionString": {
						"value": "[parameters('ls_employee_file_data_connectionString')]"
					},
					"ls_hrdata_files_connectionString": {
						"value": "[parameters('ls_hrdata_files_connectionString')]"
					},
					"ls_sql_employee_data_password": {
						"value": "[parameters('ls_sql_employee_data_password')]"
					},
					"ls_sql_hrdata_password": {
						"value": "[parameters('ls_sql_hrdata_password')]"
					},
					"ls_sql_employee_data_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_employee_data_properties_typeProperties_server')]"
					},
					"ls_sql_employee_data_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_employee_data_properties_typeProperties_database')]"
					},
					"ls_sql_employee_data_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_employee_data_properties_typeProperties_userName')]"
					},
					"ls_sql_hrdata_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_hrdata_properties_typeProperties_server')]"
					},
					"ls_sql_hrdata_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_hrdata_properties_typeProperties_database')]"
					},
					"ls_sql_hrdata_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_hrdata_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}